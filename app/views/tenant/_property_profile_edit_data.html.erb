<div class="col-md-10" id="edit_details" style="<%if @data%>display:none;<%end%>;">
        <%= form_for :property, :html => {:id => "property_form_id"}, :url => save_property_details_path(:format => :js) do |f| %>
          <div id="property_detail">
              <div class="input-group">
                <span class="input-group-addon">Description</span>
                <%= f.text_field :description, :value => @data.try(:description), :placeholder => "one-line description", class: "form-control"%>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Rent amount</span>
                <%= f.text_field :rent, :value => @data.try(:rent), :placeholder => "rent amount", class: "form-control" %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Deposit amount</span>
                <%= f.text_field :deposit, :value => @data.try(:deposit), :placeholder => "deposit amount", class: "form-control" %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Locality</span>
                <%= f.text_field :area, :value => @data.try(:area), :placeholder => "your name", class: "form-control" %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Pin Code</span>
                <%= f.text_field :pin_code, :value => @data.try(:pin_code), :placeholder => "pin code", class: "form-control" %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Postal Address</span>
                <div class="span6">
                  <textarea id="review_content" rows="5" style="width: 100%; border: 1px solid #ccc; padding: 4px;"></textarea>
                </div>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Landmark</span>
                <%= f.text_field :landmark, :value => @data.try(:landmark), :placeholder => "landmark", class: "form-control" %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">City</span>
                <%= f.text_field :city, :value => @data.try(:city), :placeholder => "city", class: "form-control" %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">State</span>
                <%= f.select 'state', options_for_select(["Tamil Nadu","Maharashtra"]), {prompt: "-- Select --"}, {:id => "state",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Built up area</span>
                <%= f.text_field :built_up_area, :value => @data.try(:built_up_area), :placeholder => "built up area", class: "form-control" %>
                <span class="input-group-addon"><small>sq. feet</small></span>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Lease Type</span>
                <%= f.select 'lease_type', options_for_select(["Family only","Bachelors","Spinsters","Available to all"]), {prompt: "-- Select --"}, {:id => "lease_type",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Water Supply</span>
                <%= f.select(:water_supply_type, [['Boring Water', 1], ['Supply Water', 2]], {prompt: "-- Select --"}, {:id => "water_supply_type",:class => 'form-control'}) %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Direction facing</span>
                <%= f.select 'direction_facing', options_for_select(["North","East","South","West"]), {prompt: "-- Select --"}, {:id => "direction_facing",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Furnishing</span>
                <%= f.select 'furnishing', options_for_select(["Fully-furnished","Semi-furnished","Unfurnished"]), {prompt: "-- Select --"}, {:id => "furnishing",:class => 'form-control'}%>
              </div>
          </div>
          <div id="furnish_detail">
              <div class="input-group">
                <span class="input-group-addon">No. of Bedroom</span>
                <%= f.select 'bedroom', options_for_select([1,2,3,4,5,"more"]), {prompt: "-- Select --"}, {:id => "bedroom",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Bathroom</span>
                <%= f.select 'bathroom', options_for_select([1,2,3,4,5,"more"]), {prompt: "-- Select --"}, {:id => "bathroom",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Wardrobe</span>
                <%= f.select 'wardrobe', options_for_select(["Yes","No"]), {prompt: "-- Select --"}, {:id => "tv",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">AC</span>
                <%= f.select 'ac', options_for_select([1,2,3,4,5,"more"]), {prompt: "-- Select --"}, {:id => "ac",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">TV</span>
                <%= f.select 'tv', options_for_select([1,2,3,4,5,"more"]), {prompt: "-- Select --"}, {:id => "tv",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Dining Table</span>
                <%= f.select 'dining_table', options_for_select(["Yes",1],["No",0]), {prompt: "-- Select --"}, {:id => "dining_table",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Parking</span>
                <%= f.select 'parking', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "parking",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Refrigerator</span>
                <%= f.select 'refrigerator', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "refrigerator",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Geyser</span>
                <%= f.select 'geyser', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "refrigerator",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Sofa</span>
                <%= f.select 'sofa', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "sofa",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Gas pipeline</span>
                <%= f.select 'gas_pipeline', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "gas_pipeline",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Gymnasium</span>
                <%= f.select 'gym', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "refrigerator",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Swimming pool</span>
                <%= f.select 'swimming_pool', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "swimming_pool",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Lift</span>
                <%= f.select 'lift', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "lift",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Washing Machine</span>
                <%= f.select 'washing_machine', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "washing_machine",:class => 'form-control'} %>
              </div>
              <div class="input-group">
                <span class="input-group-addon">Microwave</span>
                <%= f.select 'microwave', options_for_select([["Yes",1],["No",0]]), {prompt: "-- Select --"}, {:id => "microwave",:class => 'form-control'} %>
              </div>
        </div><br>
      <center><%= f.submit "Submit", :id => "property_details_next", :name => "save_property", class: "btn btn-primary" %></center>
      <% end %>
    </div>